{
  "name": "vue-typinggame",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "prepare": "cd .. && husky app/.husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "tests/*.test.{ts?(x)}": [
      "vitest run"
    ]
  },
  "dependencies": {
    "@mdi/font": "6.2.95",
    "axios": "^1.6.8",
    "roboto-fontface": "*",
    "vue": "^3.4.21",
    "vuetify": "^3.5.8"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@eslint/js": "^9.3.0",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vue-test-utils": "^1.0.1",
    "globals": "^15.2.0",
    "happy-dom": "^14.11.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pinst": "^3.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.9.0",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.5",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.6"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
}
